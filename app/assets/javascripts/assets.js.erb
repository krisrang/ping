window.assetPath = (function(){
  // TODO: automate this to grab from the manifest, Rails voodoo should be able to get it
  var map = {
    'logo.png': <%= asset_path('logo.png').inspect.html_safe %>,
    'logo_w.png': <%= asset_path('logo_w.png').inspect.html_safe %>,
    'icon_available.png': <%= asset_path('icon_available.png').inspect.html_safe %>,
    'icon_away.png': <%= asset_path('icon_away.png').inspect.html_safe %>,
    'icon_dnd.png': <%= asset_path('icon_dnd.png').inspect.html_safe %>
  };

  return function(asset){ return map[asset]; };
})();