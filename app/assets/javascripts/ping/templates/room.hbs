    <li class="message">
      <img width="32" height="32" src="//www.gravatar.com/avatar/72555f0f9d510bcd6eb2030507402655.png?s=32&amp;r=pg&amp;d=identicon" class="avatar">
      <div class="details">krisrang   â€¢  <time>1 minute ago</time></div>
      <div class="body">{{message}}</div>
    </li>
    {{/each}}
  </ul>
</div>

<div class="room-composer">
  {{boundAvatar currentUser imageSize="medium"}}
</div>

